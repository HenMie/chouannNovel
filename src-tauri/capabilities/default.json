{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://api.openai.com/**" },
        { "url": "https://generativelanguage.googleapis.com/**" },
        { "url": "https://api.anthropic.com/**" },
        { "url": "https://**" }
      ]
    }
  ]
}
